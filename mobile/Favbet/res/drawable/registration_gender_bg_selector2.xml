<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <selector>
            <item android:state_selected="true">
                <shape android:shape="rectangle">
                    <solid android:color="?General_body_profile" />
                    <corners android:radius="@dimen/default_corned_radius" />
                    <stroke android:width="1.0dip" android:color="@color/registration_gender_bg_color_selector" />
                </shape>
            </item>
            <item android:alpha="0.3">
                <shape android:shape="rectangle">
                    <solid android:color="?General_body_profile" />
                    <corners android:radius="@dimen/default_corned_radius" />
                    <stroke android:width="1.0dip" android:color="@color/registration_gender_bg_color_selector" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>