<script>
  window.addEventListener('load', Offices, false);

  function Offices() {
    var _headers = document.getElementsByTagName('h5');
    for (var i = 0; i < _headers.length; i++) {
      _headers[i].onclick = function() {
        var _offices = this.nextSibling.nextSibling;
        if (_offices.style.display === 'block') {
          this.style.color = '#ffffff';
          _offices.style.display = 'none';
        } else {
          this.style.color = 'red';
          _offices.style.display = 'block';
        }
      }
    }
  }
</script>
